!! URxvt Appearance
!URxvt.font: xft:MesloLGS NF:style=Regular:size=9
!URxvt.boldFont: xft:MesloLGS NF:style=Bold:size=9
!URxvt.italicFont: xft:MesloLGS NF:style=Italic:size=9
!URxvt.boldItalicfont: xft:MesloLGS NF:style=Bold Italic:size=9
URxvt.font: xft:mononoki Nerd Font Mono:style=Regular:size=12,xft:Noto Color Emoji:style=Regular:size=10
URxvt.boldFont: xft:mononoki Nerd Font Mono:style=Bold:size=12
URxvt.italicFont: xft:mononoki Nerd Font Mono:style=Italic:size=12
URxvt.boldItalicfont: xft:mononoki Nerd Font Mono:style=Bold Italic:size=12
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x32
URxvt.internalBorder: 24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.iso14755: false

!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D

!! Setup URLs
URxvt.urlLauncher: brave
URxvt.underlineURLs: true
URxvt.urlButton: 1

!! transparancy
URxvt*depth: 32
URxvt*background: [88]#2b3339

!! Font settings
Xft.dpi: 96

